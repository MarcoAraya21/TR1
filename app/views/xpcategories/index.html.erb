<p id="notice"><%= notice %></p>

<h1>Listing Pcategories</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pcategories.each do |pcategory| %>
      <tr>
        <td><%= pcategory.Nombre %></td>
        <td><%= link_to 'Show', pcategory %></td>
        <td><%= link_to 'Edit', edit_pcategory_path(pcategory) %></td>
        <td><%= link_to 'Destroy', pcategory, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Pcategory', new_pcategory_path %>
